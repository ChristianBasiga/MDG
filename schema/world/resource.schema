package mdg_schema.game.resource;

enum ResourceType{
    MINERAL = 1;
}

// Resource type never changes.
// split cold and hot data.
// Cold data: max occupancy, resource_type,
// Hot Data: occupants, occupied, also redundant, as occupied can be checked by simply checking length of occupants.

component Resource{
    id = 526;
    int32 maximum_occupancy = 1;
    ResourceType resource_type = 3;
}

